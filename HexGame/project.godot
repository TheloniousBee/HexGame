; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="The Travelling Hexagon"
run/main_scene="res://scene/Master.tscn"
boot_splash/image="res://bg/splash_v1.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0.686275, 0.886275, 0.905882, 1 )
config/icon="res://sprite/key.png"

[autoload]

Global="*res://script/Global.gd"

[display]

window/size/width=1920
window/size/height=1080
mouse_cursor/custom_image="res://mouse/normal_pointer.png"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": true,
"flags/anisotropic": false,
"flags/filter": true,
"flags/mipmaps": true,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}
